
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_AppLayout.cshtml";
}



@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_AppLayout.cshtml";
}
<script src="~/AngularJS/AddMember/jsEditMemberController.js"></script>
<script src="~/AngularJS/AjsFactory/AjsFactory.js"></script>

<div ng-controller="AddEditController">

    <div class="row page-tilte align-items-center">
        <div class="col-md-auto">


        </div>
        <div class="col controls-wrapper mt-3 mt-md-0 d-none d-md-block ">
            <div class="controls d-flex justify-content-center justify-content-md-end">

            </div>
        </div>
    </div>

    <div class="col-lg-12">
        <div class="card mb-4">
            <div class="card-header">
                <h4> Edit Member </h4>
            </div>

            <div class="card-body">

                <form name="formx" ng-submit="submitForm()">
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label for="inputmember4">Member Id</label>
                            <input type="text" class="form-control" id="txtMember" placeholder="Member Id" ng-disabled="true" ng-model="MemberId" />
                        </div>
                        <div class="form-group col-md-6">
                            <label for="inputreceiptno">Receipt No</label>
                            <input type="text" class="form-control" id="txtreceiptno" placeholder="Receipt No" ng-disabled="true" ng-model="ReceiptNo" />
                        </div>
                        <div class="form-group col-md-6">
                            <label for="inputfirst4">Member First Name</label>
                            <input type="text" class="form-control" id="txtFirstName" placeholder="First Name" required ng-model="FirstName" />
                        </div>
                        <div class="form-group col-md-6">
                            <label for="inputlast4">Member Last Name</label>
                            <input type="text" class="form-control" id="txtLastName" placeholder="Last Name" ng-model="LastName" />
                        </div>
                        <div class="form-group col-md-6">
                            <label for="inputDateofbirth">Date Of Birth</label>
                            <input type="date" class="form-control" ng-model="dateofbirth" ng-change="getAge(dateofbirth)" required />
                        </div>
                        <div class="form-group col-md-6">
                            <label for="inputgender">Gender</label>
                            <select class="form-control" required ng-model="Gender" ng-options="item.id as item.name for item in Genderx">
                                <option value="">--Select Gender-- </option>
                            </select>
                        </div>


                        <div class="form-group col-md-6">
                            <label for="inputage">Age</label>
                            <input type="number" class="form-control" id="txtage" ng-model="age" />
                        </div>
                        <div class="form-group col-md-6">
                            <label for="inputQualification">Qualification</label>
                            <select class="form-control" required ng-model="qualification" ng-options="item.id as item.name for item in qualificationx">
                                <option value="">--Select Qualification-- </option>
                            </select>
                            </div>
                        <div class="form-group col-md-6">
                            <label for="inputReligion">Religion</label>
                            <select class="form-control" required ng-model="religion" ng-options="item.id as item.name for item in religionx">
                                <option value="">--Select Religion-- </option>
                            </select>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="inputmaritalstatus">Marital Status</label>
                            <select class="form-control" required ng-model="maritalstatus" ng-options="item.id as item.name for item in maritalstatusx">
                                <option value="">--Select Marital Status-- </option>
                            </select>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="inputBranch4">Branch Name</label>

                            <select class="form-control" id="ddlBranch" ng-model="BranchName">
                                <option value="">Select Branch...</option>
                                <option value="1">Maharastra</option>
                                <option value="2">Madhya Pradesh</option>
                                <option value="3">Gujrat</option>
                            </select>
                        </div>

                        <div class=" form-group col-md-6">
                            <label for="inputprofession">Marital Status</label>
                            <select class="form-control" required ng-model="profession" ng-options="item.id as item.name for item in professionx">
                                <option value="">--Select Marital Status-- </option>
                            </select>
                            </div>
                            <div class=" form-group col-md-6">
                              <label for="inputprofession">Profession</label>
                                <select id="txtprofession" class="form-control" ng-model="">
                                    <option value="">Select Profession...</option>
                                    <option value="1">Doctor</option>
                                    <option value="2">Teacher</option>
                                    <option value="3">Engineer</option>
                                    <option value="4">Artist</option>
                                </select>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputcategory">Category</label>
                                <select id="txtcategory" class="form-control" ng-change="getAmount(category)" ng-model="category">
                                    <option value=""> Select Category...</option>
                                    <option value="70000.00">Full Member</option>
                                    <option value="5000.00">Associate Member</option>
                                    <option value="3000.00">Student Member</option>
                                    <option value="4000.00">Youth Member</option>
                                    <option value="3500.00">Transient Member</option>
                                </select>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputamount">Amount</label>
                                <input type="number" placeholder="00.0" class="form-control" id="txtamount" required ng-model="amount" />
                            </div>

                            <div class="form-group col-md-6">
                                <label for="inputpaidby">Paid By</label>
                                <select id="txtpaidby" class="form-control" required ng-model="paidby">
                                    <option value="0">Select Type...</option>
                                    <option value="1">Cash</option>
                                    <option value="2">Cheque</option>
                                    <option value="3">DD</option>
                                </select>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputchequeno">cheque Number</label>
                                <input type="text" class="form-control" ng-disabled="paidby != 2" id="txtchequeno" placeholder="Cheque No" ng-model="chequeno" />
                            </div>

                            <div class="form-group col-md-6" ng-hide="paidby != 2">
                                <label for="inputbankname">Bank Name</label>
                                <input type="text" class="form-control" id="txtbankname" placeholder="Bank Name" ng-model="bankname" />
                            </div>

                            <div class="form-group col-md-6" ng-hide="paidby != 2">
                                <label for="inputpayDate"> Date</label>
                                <input type="date" class="form-control" ng-model="paydate" />
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputGSTNo">GST No.</label>
                                <input type="text" class="form-control" id="txtGSTNO" placeholder="GST Number" ng-model="gstno" />
                            </div>
                            @*<div class="form-row">*@
                            <div class="form-group col-md-3">
                                <label for="inputstartdate">Start Date</label>
                                <input type="date" class="form-control" ng-model="form.startdate" ng-change='checkErr(form.startdate,form.expirydate)' />
                            </div>
                            <div class="form-group col-md-3">
                                <label for="inputExpiryDate">Expiry Date</label>
                                <input type="date" class="form-control" ng-model="form.expirydate" ng-change='checkErr(form.startdate,form.expirydate)' />
                            </div>
                            @*</div>*@

                            <div class="form-group col-md-6">
                                <label for="inputoffTelNo">Office Tel No.</label>
                                <input type="text" class="form-control" id="txtofctelno" placeholder=" Office Tel No." ng-model="officetelno" />
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputresidentialtelno">Residential Tel No.</label>
                                <input type="text" class="form-control" id="txtresidentialtelno " placeholder="Residential Tel No." ng-model="residentialtelNo" />
                            </div>
                            @*<div class="form-row">*@
                            <div class="form-group  col-md-6">
                                <label for="inputspecial interest">Special Interest </label>
                                <select class="form-control" id="txtspecialinterest " ng-model="pecialinterest">
                                    <option value="">Select Special Interest </option>
                                    <option value="1">Basketball</option>
                                    <option value="2">Table Tennis</option>
                                    <option value="3">Swimming</option>
                                    <option value="4">Fitness Centre</option>
                                    <option value="5">Roller Skating</option>
                                </select>
                                <br />

                            </div>

                            <div class="form-group col-md-6">
                                <label for="inputpanno">PAN No. </label>
                                <input type="text" class="form-control" id="txtpanno" placeholder="PAN Number" ng-model="panno" />
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputEmail4"> Email</label>
                                <input type="email" class="form-control" id="txtBranchEmail" placeholder="Email" ng-model="Email" required />


                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputPhone4">Phone No.</label>
                                <input type="number" class="form-control" id="txtPhoneNumber" placeholder="Phone Number" maxlength="10" ng-model="Phone" required />
                            </div>
                        </div>
                    <div class="form-row">
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-9">
                            <label for="inputAddress">Permanent Address</label>
                            <input type="text" class="form-control" id="txtAddress" placeholder="1234 Main St" ng-model="Address1">
                        </div>


                        <div class="form-group col-md-5">
                            <label for="inputCity">City</label>
                            <select class="form-control" id="txtCity" ng-model="pCity">
                                <option value="">Select City...</option>
                                <option value="1">Mumbai</option>
                                <option value="2">Nagpur</option>
                                <option value="3">other</option>
                            </select>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="inputState">State</label>
                            <select id="txtState" class="form-control" ng-model="pState">
                                <option value="">Select State...</option>
                                <option value="1">Maharastra</option>
                                <option value="2">Madhya Pradesh</option>
                                <option value="3">Gujrat</option>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="inputpin">Pin Code</label>
                            <input type="text" class="form-control" id="txtpin" placeholder="Enter Pin Code" ng-model="pZip">
                        </div>
                    </div>
                    <br />  <br />

                    <button type="submit" class="btn btn-primary">Edit</button>


                </form>

            </div>
        </div>

    </div>




</div>



